<%
	String admin_id = (String) session.getAttribute("admin");
	boolean login_success = admin_id == null ? false : true;
%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">	
